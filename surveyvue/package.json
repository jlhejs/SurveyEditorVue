{
  "scripts": {
    "start": "npm run build_dev && live-server --port=7777",
    "serve": "live-server --port=7777",
    "test": "karma start",
    "testsingle": "karma start --single-run",
    "testcafe": "concurrently \"http-server\" \"testcafe chrome testCafe/\"",
    "testcafe_ci": "http-server --silent & testcafe chrome:headless testCafe/ --reporter minimal",
    "testcafe_sauce": "testcafe \"saucelabs:Chrome@56.0:Windows 10\" testCafe/",
    "release": "standard-version --message \"Release: %s\" ",
    "build_knockout_dev": "webpack --env.buildType dev --env.platform knockout",
    "build_react_dev": "webpack --env.buildType dev --env.platform react",
    "build_angular_dev": "webpack --env.buildType dev --env.platform angular",
    "build_jquery_dev": "webpack --env.buildType dev --env.platform jquery",
    "build_core_dev": "webpack --env.buildType dev --env.platform core",
    "build_vue_dev": "webpack --env.buildType dev --env.platform vue",
    "build_dev": "concurrently \"npm run build_knockout_dev\" \"npm run build_react_dev\" \"npm run build_angular_dev\" \"npm run build_jquery_dev\" \"npm run build_vue_dev\" \"npm run build_core_dev\"",
    "build_knockout_prod": "webpack --env.buildType prod --env.platform knockout",
    "build_react_prod": "webpack --env.buildType prod --env.platform react",
    "build_angular_prod": "webpack --env.buildType prod --env.platform angular",
    "build_jquery_prod": "webpack --env.buildType prod --env.platform jquery",
    "build_core_prod": "webpack --env.buildType prod --env.platform core",
    "build_vue_prod": "webpack --env.buildType prod --env.platform vue",
    "build_prod": "npm run build_dev && concurrently \"npm run build_knockout_prod\" \"npm run build_react_prod\" \"npm run build_angular_prod\" \"npm run build_jquery_prod\" \"npm run build_vue_prod\" \"npm run build_core_prod\"",
    "doc_gen": "node doc_generator/lib_docgenerator.js src/entries/chunks/model.ts",
    "doc_update": "chmod +x ./docupdate_npm.sh && ./docupdate_npm.sh",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "watch_knockout_dev": "webpack --env.buildType dev --env.platform knockout --watch",
    "watch_react_dev": "webpack --env.buildType dev --env.platform react --watch",
    "watch_angular_dev": "webpack --env.buildType dev --env.platform angular --watch",
    "watch_jquery_dev": "webpack --env.buildType dev --env.platform jquery --watch",
    "watch_vue_dev": "webpack --env.buildType dev --env.platform vue --watch"
  },
  "version": "1.1.26",
  "name": "survey-library",
  "private": true,
  "devDependencies": {
    "@types/angular": "1.6.2",
    "@types/jquery": "2.0.39",
    "@types/lodash": "4.14.121",
    "@types/node": "7.0.4",
    "@types/qunit": "2.0.31",
    "@types/react": "16.4.2",
    "@types/react-dom": "16.8.0",
    "ajv": "4.11.2",
    "concurrently": "3.1.0",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",
    "dts-bundle": "0.7.2",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "generate-json-webpack-plugin": "0.2.1",
    "github-api": "^3.0.0",
    "html-loader": "0.4.4",
    "http-server": "0.9.0",
    "husky": "^1.2.1",
    "jquery": "3.1.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-junit-reporter": "1.0.0",
    "karma-qunit": "1.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "knockout": "3.5.0",
    "live-server": "1.2.0",
    "node-sass": "^4.12.0",
    "node-uuid": "1.4.7",
    "pegjs": "^0.10.0",
    "prettier": "~1.12.0",
    "puppeteer": "^1.0.0",
    "qunitjs": "2.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "replace-in-file": "^3.1.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "standard-version": "4.0.0",
    "style-loader": "0.13.1",
    "surveyjs-doc-generator": "git+https://github.com/surveyjs/surveyjs-doc-generator.git",
    "testcafe": "0.17.2",
    "testcafe-browser-provider-saucelabs": "1.1.0",
    "ts-loader": "2.3.4",
    "ts-node": "3.3.0",
    "ts-pegjs": "^0.2.3",
    "tslint": "4.5.1",
    "typescript": "3.6.2",
    "url-loader": "^0.6.2",
    "val-loader": "0.5.0",
    "vue": "2.1.10",
    "vue-class-component": "5.0.0",
    "vue-loader": "14.1.0",
    "vue-property-decorator": "4.0.0",
    "vue-template-compiler": "2.1.10",
    "webpack": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run testsingle"
    }
  }
}
